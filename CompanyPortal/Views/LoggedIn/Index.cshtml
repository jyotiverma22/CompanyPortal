

@{
    ViewBag.Title = "CompanyPortal-" + Session["username"];
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}



<div class="container setheight">

    <div class="row">

        @if (Convert.ToString(Session["role"]) == "Member" || Convert.ToString(Session["role"]) == "Admin")
        {
            Html.RenderAction("MemberPartial");

        }
        else if (Convert.ToString(Session["role"]) == "P_Manager")
        {
            Html.RenderAction("ProjManagerPartial");
        }
        else if (Convert.ToString(Session["role"]) == "R_Manager")
        {
            Html.RenderAction("RepManagerPartial");
        }

    </div>

</div>


<script type="text/javascript">
    $(document).ready(function () {
        debugger
    });

</script>




